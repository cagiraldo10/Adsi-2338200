Proceso sin_titulo
	//11. Hacer un algoritmo que lea dos números enteros A y B y muestre su diferencia.
	Definir A,B,diferencia Como Real;
	//salida
	Escribir "Digite el número A";
	//entrada
	Leer A;
	//salida
	Escribir "Digite el número B";
	//entrada
	Leer B;
	//Proceso 
	diferencia=A-B;
	//salida
	Escribir "La diferencia de estos dos números es:", diferencia;
FinProceso
