Proceso sin_titulo
	//8. Hacer un algoritmo que lea el nombre de una persona y número de horas que estudia en la semana.
	//Proceso 
	Definir nombre Como cadena;
	Definir hora_dia Como Entero;
	Definir numero_horas Como Entero;
	//salida
	Escribir "-Este algoritmo te ayuda a saber cuanto es el total de numero de horas que estudias a la semana";
	Escribir "--------------------------------------------------------------------------------------------------" ;
	//salida
	Escribir "-Escribe tu nombre";
	//entrada
	Leer nombre;
	//salida
	Escribir "-Escribe el numero de horas que ves al día";
	//entrada
	Leer hora_dia;
	//Proceso 
	numero_horas= hora_dia*5;
	//salida
	Escribir "El numero de horas total que estudias a la semana es:", numero_horas;
FinProceso
