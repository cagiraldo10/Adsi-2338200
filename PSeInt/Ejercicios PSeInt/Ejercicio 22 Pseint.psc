Proceso sin_titulo
	//22. Pedir dos números y mostrarlos ordenados de mayor a menor.
	//Proceso 
	Definir n1,n2 Como Real;
	//salida
	Escribir "Escribe el primer número";
	//entrada
	Leer n1;
	//salida
	Escribir "Escribe el segundo número";
	//entrada
	Leer n2;
	//Proceso 
	Si (n1>n2) Entonces
		Escribir n1;
		Escribir n2;
	SiNo 
		Escribir n2;
		Escribir n1;
	FinSi
FinProceso
