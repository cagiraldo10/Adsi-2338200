Proceso sin_titulo
	//16. Calcular el área de un rectángulo de lados X e Y.
	//Proceso 
	Definir X,Y_1,area como Real;
	//salidaa
	Escribir "Escriba el lado X(ancho)";
	//entrada
	Leer X;
	//salida
	Escribir "Escriba el lado Y(largo)";
	//entrada
	Leer Y_1;
	//Proceso 
	area= X*Y_1;
	//salida
	Escribir "El área del rectángulo es:", area ;
FinProceso
