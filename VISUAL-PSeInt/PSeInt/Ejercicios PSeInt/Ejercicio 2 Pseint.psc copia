Proceso sin_titulo
	// 2. Hacer un algoritmo que lea dos números enteros A y B y muestre el doble de su suma.
	// Proceso 
	Definir A,B,doble_suma Como Entero;
	// salida
	Escribir 'Escriba A';
	// entrada
	Leer A;
	// salida
	Escribir 'Escriba B';
	// entrada
	Leer B;
	// Proceso 
	doble_suma <- (A+B)*2;
	// salida
	Escribir 'El doble de la suma de estos dos numeros es:',doble_suma;
FinProceso
